10  REM STARTUP - Have fun coding on Apple II !
50  DEBUG% = 0 : REM Dynamically defined by gulp build script
100 TEXT : NORMAL : D$ = CHR$(4)

200  X = PEEK(49168) : REM Clear keyboard strobe
250  PRINT D$;"BLOAD TITLE.HGR, A$2000"
300 POKE 49167, 1 : REM Enable MOUSETEXT
400 POKE 49232, 0 : REM Turn Graphics ON
500 POKE 49235, 0 : REM Turn ON MIXED MODE
550 POKE 49236, 0 : REM Turn ON PAGE 1
600 POKE 49239, 0 : REM TURN ON HIRES
700  PRINT D$;"BRUN HELLO.WORLD" : REM Load and run HELLO.WORLD
800  IF DEBUG% THEN HTAB(16) : PRINT "debug ON";

900 END
